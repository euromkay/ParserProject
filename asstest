structdef MY1{
	int x;
	float y;
	MY1 *m1;
	function : float f(int &x){
		return this.y;
	}
	function : void foo(){}
};

structdef MY2{
	int x;
	float y;
	MY1 *m1;
	function : float f(int &x){
		return this.y;
	}
	function : void foo(){}
};

function : void main(){
	MY1 x;
	MY2 y;
	float f;
	f = x.x + y.y;
	f = x.f() + y.f();
	f = x.foo() + y.z;
}